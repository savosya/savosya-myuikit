{
  "name": "@savosya/savosya-myuikit",
  "version": "0.0.13",
  "license": "ISC",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "repository": "https://github.com/savosya/lerna-storybook-uikit-starter.git",
  "scripts": {
    "new": "bin/copy-folder.sh",
    "prepareSh": "chmod +x bin/copy-folder.sh bin/build.sh",
    "build": "yarn clean && lerna run build --skip-nx-cache && bin/build.sh",
    "build:cache": "lerna run build",
    "clean": "rm -rf packages/*/{tsconfig.tsbuildinfo,build} && rm -rf build",
    "pub:root": "./bin/publish.sh"
  },
  "peerDependencies": {
    "react": "^17 || ^18",
    "react-dom": "^17 || ^18"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-external-helpers": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/runtime": "^7.22.5",
    "@oat-sa/rollup-plugin-wildcard-external": "^1.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@types/node": "^20.3.0",
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.4",
    "cross-env": "^7.0.3",
    "globby": "^13.2.0",
    "json": "^11.0.0",
    "lerna": "^7.0.0",
    "read-pkg-up": "^9.1.0",
    "rollup": "^3.25.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^3.2.0",
    "typescript": "^5.1.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }

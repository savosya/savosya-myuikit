{
  "name": "@savosya/savosya-myuikit",
  "version": "0.0.1",
  "private": true,
  "license": "ISC",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "repository": "https://github.com/savosya/lerna-storybook-uikit-starter.git",
  "scripts": {
    "new": "bash cli/create-package.sh",
    "dev": "storybook dev -p 6006",
    "preview:sb": "http-server storybook-static -p 9009",
    "build:sb": "yarn build && storybook build",
    "build": "bash bin/build.sh",
    "clean": "rm -rf packages/*/{tsconfig.tsbuildinfo,build} && rm -rf {build,storybook-static}",
    "pub:root": "bash bin/publish.sh",
    "format": "npx prettier --write .",
    "prepareSh": "chmod +x bin/prepare.sh && bash bin/prepare.sh",
    "prepare": "husky install && yarn prepareSh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{packages,cli,tools,typings,.storybook,docs}/**/*.{js,jsx,ts,tsx,json,css,scss,md,mdx}": [
      "npx prettier --write ."
    ]
  },
  "peerDependencies": {
    "react": "^17 || ^18",
    "react-dom": "^17 || ^18"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "rc-dialog": "^9.3.0",
    "rc-drawer": "^6.5.0",
    "rc-dropdown": "^4.1.0",
    "rc-select": "^14.8.1",
    "react-tooltip": "^5.18.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@oat-sa/rollup-plugin-wildcard-external": "^1.0.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@storybook/addon-essentials": "7.4.5",
    "@storybook/addon-mdx-gfm": "^7.4.5",
    "@storybook/blocks": "^7.4.5",
    "@storybook/react": "^7.4.5",
    "@storybook/react-vite": "7.4.5",
    "@storybook/testing-library": "^0.2.1",
    "@types/node": "^20.3.0",
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "globby": "^13.2.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.0",
    "json": "^11.0.0",
    "lerna": "^7.0.0",
    "lint-staged": "^14.0.1",
    "minimist": "^1.2.8",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.25",
    "postcss-discard-comments": "^6.0.0",
    "postcss-discard-empty": "^6.0.0",
    "postcss-import": "^15.1.0",
    "prettier": "^3.0.3",
    "purgecss": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "read-pkg-up": "^9.1.0",
    "rollup": "^3.25.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-ts": "^3.4.5",
    "rollup-pluginutils": "^2.8.2",
    "sass": "^1.63.6",
    "storybook": "^7.4.5",
    "typescript": "^5.1.3",
    "vite": "^4.4.2"
  },
  "browserslist": {
    "production": [
      "last 2 versions",
      "ie >= 11",
      "Android >= 5",
      "iOS >= 9"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  }
}

{
  "name": "@savosya/savosya-myuikit",
  "version": "0.0.2",
  "private": true,
  "license": "ISC",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "repository": "https://github.com/savosya/lerna-storybook-uikit-starter.git",
  "scripts": {
    "new": "bin/copy-folder.sh",
    "prepareSh": "chmod +x bin/copy-folder.sh bin/build.sh",
    "build": "yarn clean && lerna run build --skip-nx-cache && bin/build.sh",
    "build:cache": "lerna run build",
    "clean": "rm -rf packages/*/build && rm -rf build",
    "pub:patch": "RELEASE_TYPE=\"patch\" ./bin/publish.sh",
    "pub:minor": "RELEASE_TYPE=\"minor\" ./bin/publish.sh",
    "pub:major": "RELEASE_TYPE=\"major\" ./bin/publish.sh"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-external-helpers": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/runtime": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/node": "^20.3.0",
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.4",
    "cross-env": "^7.0.3",
    "json": "^11.0.0",
    "lerna": "^7.0.0",
    "read-pkg-up": "^9.1.0",
    "rollup": "^3.25.1",
    "rollup-plugin-cpy": "^2.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.1.3"
  }
}
